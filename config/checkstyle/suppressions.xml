<?xml version="1.0"?>

<!DOCTYPE suppressions PUBLIC
        "-//Checkstyle//DTD SuppressionFilter Configuration 1.2//EN"
        "https://checkstyle.org/dtds/suppressions_1_3.dtd">

<suppressions>
    <suppress checks="MagicNumber" files=".*[\\/]src[\\/](test|it|featureTests)[\\/]"/>
    <suppress files="[\\/]generated-src[\\/]" checks="[a-zA-Z0-9]*"/>
    <suppress files="[\\/]generated/source/proto[\\/]" checks="[a-zA-Z0-9]*"/>
    <suppress files=".*\\.proto$" checks="[a-zA-Z0-9]*"/>
</suppressions>
